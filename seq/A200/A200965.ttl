seq:A200965_t_1_20220530T060619 a seq:Text ;
    cnt:chars """T[n_, k_]:= (k/n) (Binomial[-1 - k + 2 n, -1 + n] HypergeometricPFQ[{k, k - n, -n}, {1/2 + k/2 - n, 1 + k/2 - n}, 1/4]);
