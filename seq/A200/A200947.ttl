seq:A200947_p_1_20230612T094722 a seq:Text ;
    cnt:chars """a:= proc(n) option remember; local m, p, r; m:=n; r:=0;
      while m>0 do
        if m=1 then r:=r+1; break fi;
        p:= prevprime(m+1); m:= m-p;
        r:= r+2^numtheory[pi](p)
      od; r
    end:
seq(a(n), n=0..52);  # _Alois P. Heinz_, Jun 12 2023"""@en .

seq:A200947_t_1_20230612T094722 a seq:Text ;
    cnt:chars "cprime[n_Integer] := If[n==0, 1, Prime[n]]; gentable[n_Integer] := (m=n; ptable={}; While[m != 0, (i = 0; While[cprime[i] <= m, i++]; j=0; While[j<i, AppendTo[ptable, 0]; j++]; ptable[[i]]=1; m=m-cprime[i-1])]; ptable); decimal[n_Integer] := (gentable[n]; Sum[2^(k - 1)*ptable[[k]], {k, 1, Length[ptable]}]); Table[decimal[n], {n, 0, 100}]"@en .

