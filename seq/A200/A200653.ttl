seq:A200653_p_1_20240402T030113 a seq:Text ;
    cnt:chars """A200653 := proc(n)
    for k from 2*n-1 to -2 by -1 do
        if isprime(n*(n+1)+k) and isprime(n*(n+1)+k+2) then
            return k;
        end if;
    end do:
    return 0 ;
end proc:
seq(A200653(n),n=1..80) ; # _R. J. Mathar_, Nov 22 2011"""@en .

seq:A200653_t_1_20240402T030113 a seq:Text ;
    cnt:chars "a[n_]:=Module[{k=0},For[m=-2, m<2n, m++, If[PrimeQ[n(n+1)+m]&&PrimeQ[n(n+1)+m+2], k=m]]; k]; Array[a,66] (* _Stefano Spezia_, Apr 01 2024 *)"@en .

