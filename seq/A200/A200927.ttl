seq:A200927_p_1_20150918T042937 a seq:Text ;
    cnt:chars """A106325 := proc(n)
    for a from n do
        if numtheory[bigomega](a) = 2 then
            return a;
        end if;
    end do:
end proc;
prevSpr := proc(n)
    for a from n by -1 do
        if numtheory[bigomega](a) = 2 then
            return a;
        end if;
    end do:
end proc;
A200927 := proc(n)
    A106325(n)-prevSpr(n) ;
end proc:
seq(A200927(n),n=4..80) ; # _R. J. Mathar_, Nov 26 2011"""@en .

