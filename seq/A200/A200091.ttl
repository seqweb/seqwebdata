seq:A200091_o_1_20231116T074332 a seq:Text ;
    cnt:chars "(GAP) Flat(List([2..14],n->List([1..Int(n/2)],k->Sum([0..k],j->(-1)^j*Binomial(k,j)*(Sum([0..j],i->Binomial(j,i)*(Binomial(n,i)*Factorial(i)*(k-j)^(n-i)))))))); # _Muniru A Asiru_, Jul 17 2018"@en .

seq:A200091_p_1_20231116T074332 a seq:Text ;
    cnt:chars """seq(seq(eval(diff((exp(x)-x-1)^k,x$n),x=0),k=1..floor(n/2)),n=2..20); # _Dennis P. Walsh_, Apr 09 2013
T := proc(n,k) local r; k!* add(binomial(n,r)*(-1)^r*Stirling2(n-r,k-r), r=0..min(n,k)); end; # _Marko Riedel_, Mar 25, 2022"""@en .

