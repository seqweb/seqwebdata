seq:A200584_t_1_20250207T164407 a seq:Text ;
    cnt:chars """a = 2; b = -4; c = 1;
f[x_] := a*x^2 + b*x + c; g[x_] := Tan[x]
Plot[{f[x], g[x]}, {x, -.1, Pi/2}, {AxesOrigin -> {0, 0}}]
r = x /. FindRoot[f[x] == g[x], {x, .2, .3}, WorkingPrecision -> 110]
RealDigits[r]   (* A200584 *)"""@en .

