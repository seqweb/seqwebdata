seq:A200673_p_1_20171126T140107 a seq:Text ;
    cnt:chars """c:=proc(n,k,j) option remember;
  if n=3 and k=2 and j=1 then RETURN(1) fi;
  if k=2 and j=1 then RETURN(c(n-1,n-2,1)) fi;
  if k=j+1 then RETURN(c(n,j+1,j-1) + c(n-1,j,j-1)) fi;
  c(n,k-1,j)+c(n-1,k-1,j)
end:
nst:=proc(n) local res,k,j;
  res:=0;
  for j to n-3 do
     for k from j+1 to n-2 do
      res:=res+j*(k-j)*c(n,k,j) od; od;
  res
end:
seq(nst(n),n=1..21);"""@en .

seq:A200673_t_1_20171126T140107 a seq:Text ;
    cnt:chars """c[n_, k_, j_] := c[n, k, j] = Which[n == 3 && k == 2 && j == 1, 1, k == 2 && j == 1, c[n - 1, n - 2, 1], k == j + 1, c[n, j + 1, j - 1] + c[n - 1, j, j - 1], True, c[n, k - 1, j] + c[n - 1, k - 1, j]];
nst[n_] := Module[{res = 0, k, j}, For[j = 1, j <= n - 3, j++, For[k = j + 1, k <= n - 2, k++, res = res + j*(k - j)*c[n, k, j]]]; res];
Array[nst, 21] (* _Jean-François Alcover_, Nov 25 2017, translated from Maple *)"""@en .

