seq:A200780_p_1_20211224T081235 a seq:Text ;
    cnt:chars """# The ORDINAL transform of a sequence a[0], a[1], a[2], ... is the sequence b[0], b[1], b[2], ... where b[n] is the number of times a[n] has occurred in [a[0], ..., a[n]].
ORDINAL:=proc(a)  local b,t1,tlist,clist,n,t,nt;
if whattype(a) <> list then RETURN([]); fi:
t1:=nops(a);
tlist:=[];
clist:=Array(1..t1,0);
b:=[]; nt:=0;
for n from 1 to t1 do t:=a[n];
if member(t,tlist,'p') then clist[p] := clist[p]+1; b:=[op(b),clist[p]];
else nt:=nt+1; tlist:=[op(tlist),t]; clist[nt]:=1; b:=[op(b),1]; fi;
od: b; end: # _N. J. A. Sloane_, Apr 09 2020
See also A200779."""@en .

