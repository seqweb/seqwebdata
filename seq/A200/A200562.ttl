seq:A200562_o_1_20240708T214250 a seq:Text ;
    cnt:chars """(PARI) {a(n) = if( n<0, polcoeff( x^3 / ((2 - x) * (4 + 3*x + x^2)) + x * O(x^-n), -n), polcoeff( 1 / ((1 - 2*x) * (1 + 3*x + 4*x^2)) + x * O(x^n), n))}; /* _Michael Somos_, Sep 17 2014 */
