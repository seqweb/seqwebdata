seq:A008275_t_1_20250216T083231 a seq:Text ;
    cnt:chars """Flatten[Table[StirlingS1[n, k], {n, 1, 10}, {k, 1, n}]] (* _Jean-FranÃ§ois Alcover_, May 18 2011 *)
Flatten@Table[Coefficient[Product[x-k, {k, 0, n-1}], x, Range[n]], {n, Range[10]}] (* _Oliver Seipel_, Jun 11 2024 *)
a[n_, n_] := 1; a[n_, 0] := 0; a[0, k_] := 0;
a[n_, k_] := a[n, k] = a[n-1, k-1] + (n-1) a[n-1, k];
