seq:A000090_o_1_20210808T113454 a seq:Text ;
    cnt:chars "(PARI) {a(n) = if( n<0, 0, n! * polcoeff( exp( -(x^3 / 3) + x*O(x^n)) / (1 - x), n))} /* _Michael Somos_, Jul 28 2009 */"@en .

seq:A000090_p_1_20210808T113454 a seq:Text ;
