seq:A000475_o_1_20230706T052903 a seq:Text ;
    cnt:chars """(PARI) x='x+O('x^66); Vec( serlaplace(exp(-x)/(1-x)*(x^4/4!)) ) \\\\ _Joerg Arndt_, Feb 19 2014
(Python)
from sympy import binomial
A000475_list, m, x = [], 1, 0
for n in range(4,100):
    x, m = x*n + m*binomial(n,4), -m
    A000475_list.append(x) # _Chai Wah Wu_, Nov 01 2014"""@en .

seq:A000475_p_1_20230706T052903 a seq:Text ;
    cnt:chars """a:=n->sum(n!*sum((-1)^k/(k-3)!, j=0..n), k=3..n): seq(-a(n)/4!, n=3..22); # _Zerinvary Lajos_, May 25 2007
G(x):=exp(-x)/(1-x)*(x^4/4!): f[0]:=G(x): for n from 1 to 26 do f[n]:=diff(f[n-1],x) od: x:=0: seq(f[n],n=4..23); # _Zerinvary Lajos_, Apr 03 2009"""@en .

