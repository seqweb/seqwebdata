seq:A000478_o_1_20220603T190410 a seq:Text ;
    cnt:chars """(PARI) Vec(x^6*(12*x^3-40*x^2+45*x-15)/((1-x)^3*(1-2*x)^2*(3*x-1))+O(x^66)) /* _Joerg Arndt_, Apr 10 2013 */
(Python) # based on _Vladimir Kruchinin_'s formula
def A000478():
    a = 15; n = 7; z = 4; s = 15;
    while True:
        yield a
        z = 2*z; s += n*(z-2) + 3; a = 3*a + s; n += 1
a = A000478(); print([next(a) for _ in range(6, 32)]) # _Peter Luschny_, Oct 04 2018"""@en .

seq:A000478_t_1_20220603T190410 a seq:Text ;
    cnt:chars """Table[(1+n+n^2)/2-(1/2+n/4)*2^n+3^n/6,{n,6,30}] (* or *) LinearRecurrence[ {10,-40,82,-91,52,-12},{15,105,490,1918,6825,22935},25] (* _Harvey P. Dale_, Jul 17 2011 *)
offset = 6; terms = 26; egf = (Exp[x]-1-x)^3/3!; Drop[CoefficientList[egf + O[x]^(terms+offset), x]*Range[0, terms+offset-1]!, offset] (* _Jean-François Alcover_, May 07 2017 *)"""@en .

