seq:A000605_o_1_20190915T134831 a seq:Text ;
    cnt:chars """(C)
int A000605(int i)
{
    const int ring = i*i;
    int result = 0;
    for (int a = -i; a <= i; a++)
        for (int b = -i; b <= i; b++)
            for (int c = -i; c <= i; c++)
                if ( ring >= a*a+b*b+c*c )  result++;
    return result;
} /* _Oskar Wieland_, Apr 08 2013 */
(PARI)
N=66;  q='q+O('q^(N^2));
t=Vec((eta(q^2)^5/(eta(q)^2*eta(q^4)^2))^3/(1-q));  /* A117609 */
vector(sqrtint(#t),n,t[(n-1)^2+1])
/* _Joerg Arndt_, Apr 08 2013 */"""@en .

