seq:A000908_p_1_20230216T145405 a seq:Text ;
    cnt:chars """U0 := (1-sqrt(1-4*x))/2/x ;
V0 := 1+x*subs(x=x^2,U0) ;
C := ( subs(x=x^2,U0)^3 -3*subs(x=x^4,U0)*subs(x=x^2,V0) -subs(x=x^6,U0) +3*subs(x=x^6,V0) )/6 ; # (19)
taylor(%,x=0,60) ;
L := gfun[seriestolist](%) ;
seq(op(2*i+1,L),i=0..(nops(L)-1)/2) ; # _R. J. Mathar_, Jul 26 2019"""@en .

seq:A000908_t_1_20230216T145405 a seq:Text ;
    cnt:chars """u0[x_] := (1 - Sqrt[1 - 4 x])/(2 x); v0[x_] := 1 + x u0[x^2];
gf = Simplify[(u0[x]^3 - 3 u0[x^2] v0[x] - u0[x^3] + 3 v0[x^3])/6]
CoefficientList[gf + O[x]^30, x] (* _Andrey Zabolotskiy_, Feb 08 2023 *)"""@en .

