seq:A000978_o_1_20250216T083222 a seq:Text ;
    cnt:chars """(PARI) forprime(p=2,5000,if(ispseudoprime(2^p\\/3),print1(p", "))) \\\\ _Charles R Greathouse IV_, Jul 15 2011
(Haskell)
a000978 n = a000978_list !! (n-1)
a000978_list = filter ((== 1) . a010051 . a001045) a065091_list
-- _Reinhard Zumkeller_, Mar 24 2013
(Python)
from gmpy2 import divexact
from sympy import prime, isprime
A000978 = [p for p in (prime(n) for n in range(2,10**2)) if isprime(divexact(2**p+1,3))] # _Chai Wah Wu_, Sep 04 2014"""@en .

