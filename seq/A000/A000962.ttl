seq:A000962_p_1_20220413T132515 a seq:Text ;
    cnt:chars """A000962:=(z+1)*(2*z**4-7*z**3+6*z**2+z-1)/(-1+7*z**2-3*z**4+z**6); # conjectured by _Simon Plouffe_ in his 1992 dissertation
