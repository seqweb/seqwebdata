seq:A000834_p_1_20250105T195131 a seq:Text ;
    cnt:chars """A000834 := exp(x)*(sin(x)+cos(x))/(cos(x)-sin(x)) : for n from 0 to 200 do printf("%d %d ",n,n!*coeftayl(A000834,x=0,n)) ; end: # _R. J. Mathar_, Nov 19 2006
