seq:A000324_o_1_20250109T080219 a seq:Text ;
    cnt:chars """(PARI) a(n)=if(n<2,max(0,1+4*n),a(n-1)^2-4*a(n-1)+4)
(PARI) a(n)=if(n<1,n==0,n=2^n;fibonacci(n+1)+fibonacci(n-1)+2)"""@en .

seq:A000324_t_1_20250109T080219 a seq:Text ;
    cnt:chars """t = {1, 5}; Do[AppendTo[t, t[[-1]]^2 - 4*t[[-1]] + 4], {n, 11}] (* _T. D. Noe_, Jun 19 2012 *)
Join[{1}, RecurrenceTable[{a[n] == a[n-1]^2 - 4*a[n-1] + 4, a[1] == 5}, a, {n, 1, 8}]] (* _Jean-François Alcover_, Feb 07 2016 *)
Join[{1},NestList[#^2-4#+4&,5,10]] (* _Harvey P. Dale_, Dec 11 2023 *)"""@en .

