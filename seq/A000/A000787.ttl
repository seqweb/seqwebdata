seq:A000787_o_1_20230113T110403 a seq:Text ;
    cnt:chars """(Python)
from itertools import count, islice, product
def ud(s): return s[::-1].translate({ord('6'):ord('9'), ord('9'):ord('6')})
def agen():
    yield from [0, 1, 8]
    for d in count(2):
        for start in "1689":
            for rest in product("01689", repeat=d//2-1):
                left = start + "".join(rest)
                right = ud(left)
                for mid in [[""], ["0", "1", "8"]][d%2]:
                    yield int(left + mid + right)
print(list(islice(agen(), 47))) # _Michael S. Branicky_, Mar 29 2022"""@en .

seq:A000787_t_1_20230113T110403 a seq:Text ;
    cnt:chars "fQ[n_] := Block[{s = {0, 1, 6, 8, 9}, id = IntegerDigits[n]}, If[ Union[ Join[s, id]] == s && (id /. {6 -> 9, 9 -> 6}) == Reverse[id], True, False]]; Select[ Range[0, 16190], fQ[ # ] &] (* _Robert G. Wilson v_, Oct 11 2005 *)"@en .

