seq:A000630_e_1_20211219T093934 a seq:Text ;
    cnt:chars "E.g. 4 = 1+1+1+1 = 2^1 + 1+1 = 2^1 + 2^1 = 2^2 = 2^1+1 = 1+2^1 + 1 = 1+1+2^1."@en .

seq:A000630_p_1_20211219T093934 a seq:Text ;
    cnt:chars """N:= 100: # to get all terms up to a(N)
F:= add(a[i]*q^i,i=0..N) - 1/(1 - add(a[i]*q^(2^i),i=0..ilog2(N))):
S:= series(F, q, N+1);
sol:= {}:
for i from 0 to N do
  eq:= subs(sol, coeff(S,q,i));
  sol:= sol union {a[i]=solve(eq,a[i])};
od:
subs(sol, [seq(a[i],i=0..N)]); # _Robert Israel_, Dec 12 2014"""@en .

