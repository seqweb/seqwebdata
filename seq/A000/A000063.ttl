seq:A000063_o_1_20160203T160242 a seq:Text ;
    cnt:chars """(PARI)
C(n)=if(type(n)==type(1), binomial(2*n,n)/(n+1), 0);
a(n)=C(floor(n/2)-1) - C(n/4-1) - C(n/6-1);
vector(66,n, a(n+4))
\\\\ _Joerg Arndt_, Aug 31 2014"""@en .

seq:A000063_t_1_20160203T160242 a seq:Text ;
    cnt:chars "c[n_Integer] := CatalanNumber[n]; c[_] = 0; a[n_] := c[Floor[n/2]-1] - c[n/4-1] - c[n/6-1]; Array[a, 40, 5] (* _Jean-Fran√ßois Alcover_, Feb 03 2016, after _Joseph Myers_ *)"@en .

