seq:A000689_o_1_20240706T210928 a seq:Text ;
    cnt:chars """(Sage) [power_mod(2,n,10)for n in range(0, 81)] #  _Zerinvary Lajos_, Nov 03 2009
(PARI) for(n=0,80, if(n,{x=(n+3)%4+1; print1(10-(4*x^3+47*x-27*x^2)/3,", ")},{print1("1, ")}))
(Magma) [2^n mod 10: n in [0..150]]; // _Vincenzo Librandi_, Apr 12 2011
(Haskell)
a000689 n = a000689_list !! n
a000689_list = 1 : cycle [2,4,8,6]  -- _Reinhard Zumkeller_, Sep 15 2011"""@en .

