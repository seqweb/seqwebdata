seq:A000271_t_1_20220908T084427 a seq:Text ;
    cnt:chars """Table[Sum[(-1)^(n - k) k! Binomial[n + k, 2 k], {k, 0, n}], {n, 0, 22}] (* _Jean-François Alcover_, Apr 11 2011, after _Paul Barry_ *)
