seq:A000737_o_1_20220612T120059 a seq:Text ;
    cnt:chars """(Sage) # Algorithm of L. Seidel (1877)
def A000737_list(n) :
    R = []; A = {-1:0, 0:0}
    k = 0; e = 1
    for i in range(n) :
        Am = i+1
        A[k + e] = 0
        e = -e
        for j in (0..i) :
            Am += A[k]
            A[k] = Am
            k += e
        # To trace the algorithm remove the comment sign.
        # print([A[z] for z in (-i//2..i//2)])
        R.append(A[e*i//2])
    return R
A000737_list(10) # _Peter Luschny_, Jun 02 2012
(Haskell)
a000737 n = sum $ zipWith (*) (a109449_row n) [1..]
-- _Reinhard Zumkeller_, Nov 05 2013
(Python)
from itertools import count, accumulate, islice
def A000737_gen(): # generator of terms
    blist = tuple()
    for i in count(1):
        yield (blist := tuple(accumulate(reversed(blist),initial=i)))[-1]
A000737_list = list(islice(A000737_gen(),40)) # _Chai Wah Wu_, Jun 12 2022"""@en .

seq:A000737_t_1_20220612T120059 a seq:Text ;
    cnt:chars """CoefficientList[Series[(1+x)*(Tan[x]+1/Cos[x])* E^x, {x, 0, 20}], x]* Range[0, 20]! (* _Vaclav Kotesovec_, Oct 02 2013 *)
