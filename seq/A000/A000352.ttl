seq:A000352_p_1_20230208T164007 a seq:Text ;
    cnt:chars """A000352:=-(-5+6*z)/(3*z-1)/(2*z-1)/(z-1)**2; # [Conjectured by _Simon Plouffe_ in his 1992 dissertation.] [correct up to offset]
# second Maple program:
a:= n-> (<<0|0|1|2>>. <<7|1|0|0>, <-17|0|1|0>, <17|0|0|1>, <-6|0|0|0>>^n)[1, 4]:
seq(a(n), n=4..30);  # _Alois P. Heinz_, Aug 26 2008"""@en .

seq:A000352_t_1_20230208T164007 a seq:Text ;
