                (3111)    (22211)     (32221)
                (21111)   (32111)     (33211)
                (111111)  (41111)     (42211)
                          (221111)    (222211)
                          (311111)    (322111)
                          (2111111)   (331111)
                          (11111111)  (421111)
                                      (511111)
