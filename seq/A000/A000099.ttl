seq:A000099_t_1_20160203T160254 a seq:Text ;
    cnt:chars "nmax = 3*10^4; A[n_] := 1 + 4*Floor[Sqrt[n]] + 4*Floor[Sqrt[n/2]]^2 + 8* Sum[Floor[Sqrt[n - j^2]], {j, Floor[Sqrt[n/2]]+1, Floor[Sqrt[n]]}]; V[n_] := Pi*n; P[n_] := A[n] - V[n]; record = 0; A000099 = Reap[For[k = 0; n = 1, n <= nmax, n++, p = Abs[P[n]]; If[p > record, record = p; k++; Sow[n]; Print[\"a(\", k, \") = \", n];]]][[2, 1]] (* _Jean-Fran√ßois Alcover_, Feb 03 2016 *)"@en .

