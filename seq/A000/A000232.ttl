seq:A000232_p_1_20250216T083220 a seq:Text ;
    cnt:chars """A000232 := proc(n)
    local k;
    for k from 1 do
        if A036262(n,k) > 2 then
            return k-1 ;
        end if;
    end do:
end proc:
seq(A000232(n),n=1..40) ; # _R. J. Mathar_, May 10 2023"""@en .

seq:A000232_t_1_20250216T083220 a seq:Text ;
    cnt:chars "max = 10^4; triangle = NestList[Abs[Differences[#]] &, Prime[Range[max]], max]; a[n_] := (p = Position[triangle[[n + 1]], k_ /; k > 2, 1, 1]; If[p == {}, Nothing, p[[1, 1]] - 1]); Table[a[n], {n, 1, Sqrt[max]}] (* _Jean-François Alcover_, Feb 06 2016 *)"@en .

