seq:A000115_o_1_20250430T100856 a seq:Text ;
    cnt:chars """(Magma) [Round((n+4)^2/20): n in [0..70]]; // _Vincenzo Librandi_, Jun 23 2011
(PARI) a(n)=(n^2+8*n+26)\\20 \\\\ _Charles R Greathouse IV_, Jun 23 2011"""@en .

seq:A000115_p_1_20250430T100856 a seq:Text ;
    cnt:chars """1/((1-x)*(1-x^2)*(1-x^5)): seq(coeff(series(%, x, n+1), x, n), n=0..65);
# next Maple program:
s:=proc(n) if n mod 5 = 0 then RETURN(1); fi; if n mod 5 = 1 then RETURN(0); fi; if n mod 5 = 2 then RETURN(1); fi; if n mod 5 = 3 then RETURN(-1); fi; if n mod 5 = 4 then RETURN(-1); fi; end: f:=n->(2*n^2+16*n+27+5*(-1)^n+8*s(n))/40: seq(f(n), n=0..65);  # from Jeger's paper"""@en .

seq:A000115_t_1_20250430T100856 a seq:Text ;
    cnt:chars """nn=50;CoefficientList[Series[1/(1-x)/(1-x^2)/(1-x^5),{x,0,nn}],x]  (* _Geoffrey Critzer_, Jan 20 2013 *)
LinearRecurrence[{1,1,-1,0,1,-1,-1,1},{1,1,2,2,3,4,5,6},70] (* _Harvey P. Dale_, Sep 27 2019 *)"""@en .

