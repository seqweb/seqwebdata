seq:A000771_t_1_20210725T112631 a seq:Text ;
    cnt:chars """lst={};Do[f=StirlingS2[n, 7];AppendTo[lst, f], {n, 7, 5!}];lst (* _Vladimir Joseph Stephan Orlovsky_, Sep 27 2008 *)
CoefficientList[Series[1/((1 - x) (1 - 2 x) (1 - 3 x) (1 - 4 x) (1 - 5 x) (1 - 6 x) (1 - 7 x)), {x, 0, 25}], x] (* _Vladimir Joseph Stephan Orlovsky_, Jun 20 2011 *)
