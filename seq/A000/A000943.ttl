seq:A000943_p_1_20141209T002556 a seq:Text ;
    cnt:chars "with(numtheory); n := 50; for d from 2 to n do C(d) := 0; for h from 1 to d+3 do if (h mod 2 = 1) and (d+3 mod h = 0) then C(d) := C(d) + phi(h) * 2^((d+3)/h); fi; od; C(d) := 2^(floor(d/2)) - floor ((d+4)/2) + C(d)/(4*(d+3)); od: A000943 := n-> eval(C(n));"@en .

seq:A000943_t_1_20141209T002556 a seq:Text ;
    cnt:chars "a[ n_ ] := 2^Floor[ n/2 ]-Floor[ (n+4)/2 ]+(1/(4*(n+3)))*Plus@@Map[ EulerPhi[ # ]*2^((n+3)/#)&, Select[ Divisors[ n+3 ], OddQ ] ]"@en .

