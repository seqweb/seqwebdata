seq:A000807_o_1_20200502T071844 a seq:Text ;
    cnt:chars """(Python)
from sympy import binomial, bell
def a(n): return sum(binomial(2*n, k)*(-1)**k*bell(k)*bell(2*n - k) for k in range(2*n  + 1))
print([a(n) for n in range(21)]) # _Indranil Ghosh_, Sep 11 2017"""@en .

seq:A000807_p_1_20200502T071844 a seq:Text ;
    cnt:chars """Bell := combinat:-bell:
A000807 := n -> add(binomial(2*n, k)*(-1)^k*Bell(k)*Bell(2*n-k), k = 0..2*n):
seq(A000807(n), n=0..17); # _Peter Luschny_, Sep 10 2017"""@en .

