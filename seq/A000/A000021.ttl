seq:A000021_o_1_20230708T011825 a seq:Text ;
    cnt:chars """(PARI) a(n)=if(n<0,0,sum(k=1,2^n,0<sum(y=0,sqrtint(k\\12),issquare(k-12*y^2))))
(PARI) a(n)=local(A);if(n<0,0,A=qfrep([1,0;0,12],2^n);sum(k=1,2^n,A[k]!=0))
(Haskell)
a000021 n = length [() | k <- [1..2^n],
        sum [a010052 (k - 12*y^2) | y <- [0..a000196 (k `div` 12)]] > 0]
-- _Reinhard Zumkeller_, Apr 16 2012"""@en .

