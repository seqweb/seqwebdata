seq:A000738_o_1_20220611T153511 a seq:Text ;
    cnt:chars """(Haskell)
a000738 n = sum $ zipWith (*) (a109449_row n) a000045_list
-- _Reinhard Zumkeller_, Nov 03 2013
(Python)
from itertools import islice, accumulate
def A000738_gen(): # generator of terms
    blist, a, b = tuple(), 0, 1
    while True:
        yield (blist := tuple(accumulate(reversed(blist),initial=a)))[-1]
        a, b = b, a+b
A000738_list = list(islice(A000738_gen(),30)) # _Chai Wah Wu_, Jun 11 2022"""@en .

seq:A000738_p_1_20220611T153511 a seq:Text ;
    cnt:chars """read(transforms);
