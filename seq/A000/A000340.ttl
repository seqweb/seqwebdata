seq:A000340_t_1_20250123T124112 a seq:Text ;
    cnt:chars """a[ n_] := MatrixPower[ {{1, 0, 0}, {1, 1, 0}, {1, 1, 3}}, n + 1][[3, 1]]; (* _Michael Somos_, May 28 2014 *)
