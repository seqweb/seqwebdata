seq:A000574_o_1_20250216T083221 a seq:Text ;
    cnt:chars """(Magma) [3*Binomial(n+2,5)-2*Binomial(n+1,5): n in [3..50]]; // _Vincenzo Librandi_, Jun 10 2012
(PARI) x='x+O('x^50); Vec(x^3*(3-2*x)/(1-x)^6) \\\\ _G. C. Greubel_, Nov 22 2017"""@en .

seq:A000574_p_1_20250216T083221 a seq:Text ;
    cnt:chars """A000574:=-(-3+2*z)/(z-1)**6; # conjectured by _Simon Plouffe_ in his 1992 dissertation
seq(3*binomial(n+2,5)-2*binomial(n+1,5),n=3..100); # _Robert Israel_, Aug 04 2015
A000574 := n -> GegenbauerC(`if`(5<n,5,2*n-5), -n, -1/2):
seq(simplify(A000574(n)), n=3..20); # _Peter Luschny_, May 10 2016"""@en .

