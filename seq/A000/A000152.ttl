seq:A000152_o_1_20181002T050705 a seq:Text ;
    cnt:chars "(PARI) first(n)=my(x='x); x+=O(x^(n+1)); Vec((2*sum(k=1,sqrtint(n),x^k^2) + 1)^16) \\\\ _Charles R Greathouse IV_, Jul 29 2016"@en .

seq:A000152_p_1_20181002T050705 a seq:Text ;
    cnt:chars """(sum(x^(m^2),m=-10..10))^16;
# Alternative:
A000152list := proc(len) series(JacobiTheta3(0, x)^16, x, len+1);
seq(coeff(%, x, j), j=0..len-1) end: A000152list(24); # _Peter Luschny_, Oct 02 2018"""@en .

