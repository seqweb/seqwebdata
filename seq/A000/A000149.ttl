seq:A000149_o_1_20240730T021648 a seq:Text ;
    cnt:chars """(PARI) for(n=0, 28, print1(floor(exp(n)), ", ")); \\\\ _Arkadiusz Wesolowski_, Nov 26 2011
(Haskell)
a000149 = floor . (exp 1 ^)
a000149_list = let e = exp 1 in map floor $ iterate (* e) 1
-- _Reinhard Zumkeller_, Mar 17 2015
(PARI) apply( A000149(n)=exp(n)\\1, [0..30]) \\\\ An error message will say so if default(realprecision) must be increased, for large n. - _M. F. Hasler_, May 27 2018
(Python)
from sympy import floor, E
def a(n):  return floor(E**n)
print([a(n) for n in range(29)]) # _Michael S. Branicky_, Jul 20 2021"""@en .

seq:A000149_t_1_20240730T021648 a seq:Text ;
    cnt:chars """a[n_]:=Floor[E^n]; (* _Vladimir Joseph Stephan Orlovsky_, Dec 12 2008 *)
