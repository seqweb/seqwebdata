seq:A000998_p_1_20201027T084808 a seq:Text ;
    cnt:chars """b:= proc(n) option remember; `if`(n<3, [0$2, 1][n+1],
      add(binomial(n-3, j)*b(j), j=0..n-3))
    end:
a:= n-> b(n+5):
seq(a(n), n=0..30);  # _Alois P. Heinz_, May 21 2019"""@en .

seq:A000998_t_1_20201027T084808 a seq:Text ;
    cnt:chars """b[n_] := b[n] = If[n<3, {0, 0, 1}[[n+1]], Sum[Binomial[n-3, j] b[j], {j, 0, n-3}]];
a[n_] := b[n+5];
a /@ Range[0, 30] (* _Jean-François Alcover_, Oct 27 2020, after _Alois P. Heinz_ *)"""@en .

