seq:A000202_o_1_20250105T195131 a seq:Text ;
    cnt:chars "(PARI) a(n) = floor((13*n - 1)/8); \\\\ _Jon E. Schoenfield_, Aug 21 2022"@en .

seq:A000202_p_1_20250105T195131 a seq:Text ;
    cnt:chars "a[0] := 0:a[1] := 1:a[2] := 3:a[3] := 4:a[4] := 6:a[5] := 8:a[6] := 9:a[7] := 11:a[8] := 12: for m from 9 to 200 do if irem(m,8)=0 then myrem := 8; myquo := iquo(m,8)-1; else myrem := irem(m,8); myquo := iquo(m,8) fi; a[m] := 13*myquo +a[myrem] od: for k from 1 to 200 do printf(`%a,`,a[k]) od: # _James A. Sellers_, May 29 2000"@en .

seq:A000202_t_1_20250105T195131 a seq:Text ;
    cnt:chars "Set[#, {1, 3, 4, 6, 8, 9, 11, 12}] &@ Map[a[#] &, Range[0, 7]]; a[n_] := a[n] = 13 #1 + a[#2] & @@ QuotientRemainder[n, 8]; Array[a, 68, 0] (* _Michael De Vlieger_, Sep 08 2017 *)"@en .

