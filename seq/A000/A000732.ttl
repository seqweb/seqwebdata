seq:A000732_o_1_20220612T120055 a seq:Text ;
    cnt:chars """(Haskell)
a000732 n = sum $ zipWith (*) (a109449_row n) a008578_list
(Python)
from itertools import accumulate, count, islice
from sympy import prime
def A000732_gen(): # generator of terms
    yield 1
    blist = (1,)
    for i in count(1):
        yield (blist := tuple(accumulate(reversed(blist),initial=prime(i))))[-1]
A000732_list = list(islice(A000732_gen(),40)) # _Chai Wah Wu_, Jun 12 2022"""@en .

seq:A000732_t_1_20220612T120055 a seq:Text ;
