seq:A000551_p_1_20170417T144636 a seq:Text ;
    cnt:chars "# ht(m) counts trees of height<=m; ht := proc(m) local i; [ T0,{seq(T.i=Prod(Z,Set(T.(i+1))),i=0..m-1),T.m=Z},labeled ] end: M[ 4220 ] := n->count(ht(2),size=n)-count(ht(1),size=n): seq(M[ 4220 ](n), n=3..21);"@en .

seq:A000551_t_1_20170417T144636 a seq:Text ;
    cnt:chars """a[n_] = n*(n-1)*Sum[k^(n-2-k)*Binomial[n-2, k-1], {k, n-2}]; Table[a[n], {n, 3, 21}] (* _Jean-Fran√ßois Alcover_, Jun 29 2011, after formula *)
