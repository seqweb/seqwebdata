seq:A000603_o_1_20240718T142928 a seq:Text ;
    cnt:chars """(Haskell)
a000603 n = length [(x,y) | x <- [0..n], y <- [0..n], x^2 + y^2 <= n^2]
-- _Reinhard Zumkeller_, Jan 23 2012
(PARI) a(n)=my(n2=n^2);sum(a=0,n,sqrtint(n2-a^2)+1) \\\\ _Charles R Greathouse IV_, Apr 03 2013
(Python)
from math import isqrt
def A000603(n): return (m:=n<<1)+sum(isqrt(k*(m-k)) for k in range(1,n))+1 # _Chai Wah Wu_, Jul 18 2024"""@en .

seq:A000603_t_1_20240718T142928 a seq:Text ;
    cnt:chars """Table[cnt = 0; Do[If[x^2 + y^2 <= n^2, cnt++], {x, 0, n}, {y, 0, n}]; cnt, {n, 0, 51}] (* _T. D. Noe_, Apr 02 2013 *)
Table[If[n==1,1,2*Sum[Sum[A255195[[n, n - k + 1]], {k, 1, k}], {k, 1, n}] - Ceiling[(n - 1)/Sqrt[2]]],{n,1,52}] (* _Mats Granvik_, Feb 19 2015 *)"""@en .

