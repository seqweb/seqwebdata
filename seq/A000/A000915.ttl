seq:A000915_o_1_20221021T220755 a seq:Text ;
    cnt:chars """(PARI) {a(n) = n * (n+1) * (n+2) * (n+3) * (n+4) * (15*n^3+ 150*n^2 + 485*n + 502) / 5760}; /* _Michael Somos_, Sep 04 2017 */
(Sage) [stirling_number1(n,n-4) for n in range(5, 30)] # _Zerinvary Lajos_, May 16 2009"""@en .

seq:A000915_p_1_20221021T220755 a seq:Text ;
    cnt:chars """A000915 := proc(n)
    combinat[stirling1](n+4,n) ;
end proc:
seq(A000915(n),n=1..10) ; # _R. J. Mathar_, May 19 2016"""@en .

seq:A000915_t_1_20221021T220755 a seq:Text ;
    cnt:chars """Table[Binomial[n + 4, 5]*(15*n^3 + 150*n^2 + 485*n + 502)/48, {n, 50}] (* _T. D. Noe_, Jun 20 2012 *)
a[ n_] := n (n + 1) (n + 2) (n + 3) (n + 4) (15 n^3 + 150 n^2 + 485 n + 502) / 5760; (* _Michael Somos_, Sep 04 2017 *)"""@en .

