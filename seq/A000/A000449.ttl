seq:A000449_o_1_20230706T053621 a seq:Text ;
    cnt:chars """(PARI) my(x='x+O('x^66)); Vec( serlaplace(exp(-x)/(1-x)*(x^3/3!)) ) \\\\ _Joerg Arndt_, Feb 19 2014
(Python)
A000449_list, m, x = [], 1, 0
for n in range(3,21):
    x, m = x*n + m*(n*(n-1)*(n-2)//6), -m
    A000449_list.append(x) # _Chai Wah Wu_, Sep 23 2014"""@en .

seq:A000449_p_1_20230706T053621 a seq:Text ;
    cnt:chars """# with k fixed-points:
G:=exp(-z)*z^k/((1-z)*k!: Gser:=series(G,z,21):
for n from k to 20 do a(n)=n!*coeff(Gser,z,n): end do: # _Paul Weisenhorn_, May 30 2010"""@en .

