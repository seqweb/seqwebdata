seq:A001001_o_1_20250410T220425 a seq:Text ;
    cnt:chars """(PARI)
N=17; default(seriesprecision,N); x=z+O(z^(N+1))
c=sum(j=1,N,j*x^j);
t=1/prod(j=1,N, eta(x^(j))^j)
t=log(t)
t=serconvol(t,c)
Vec(t)
/* _Joerg Arndt_, May 03 2008 */
(PARI) a(n)=sumdiv(n,d, d * sumdiv(d,t, t ) );  /* _Joerg Arndt_, Oct 07 2012 */
(PARI) a(n)=sumdivmult(n,d, sigma(d)*d) \\\\ _Charles R Greathouse IV_, Sep 09 2014
(Haskell)
a001001 n = sum [sum [k * (if k `mod` l == 0 then l else 0) | k <- [1..n], n `mod` k == 0 ] | l <- [1..n]]
a = [ a001001 n | n <- [1..53]]
putStrLn $ concat $ map (++ ", ") (map show a) -- _Miles Wilson_, Apr 04 2025"""@en .

seq:A001001_p_1_20250410T220425 a seq:Text ;
    cnt:chars """nmax := 100:
L12 := [seq(1,i=1..nmax) ];
L27 := [seq(i,i=1..nmax) ];
L290 := [seq(i^2,i=1..nmax) ];
DIRICHLET(L12,L27) ;
DIRICHLET(%,L290) ; # _R. J. Mathar_, Sep 25 2017"""@en .

seq:A001001_t_1_20250410T220425 a seq:Text ;
    cnt:chars """a[n_] := Sum[ d*DivisorSigma[1, d], {d, Divisors[n]}]; Table[ a[n], {n, 1, 42}] (* _Jean-François Alcover_, Jan 20 2012, after _Vladeta Jovovic_ *)
f[p_, e_] := Product[(p^(e + k) - 1)/(p^k - 1), {k, 1, 2}]; a[1] = 1; a[n_] := Times @@ (f @@@ FactorInteger[n]); Array[a, 100] (* _Amiram Eldar_, Aug 29 2019 *)"""@en .

